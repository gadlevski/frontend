{
  "name": "frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env port=3200",
    "start:vite": "vite",
    "dev": "concurrently \"npm start\" \"npm run server\"",
    "dev:vite": "concurrently \"npm run start:vite\" \"npm run server\"",
    "server": "node ./json-server/index.js",
    "b:prod": "webpack --env mode=production",
    "b:prod:netlify": "webpack --env mode=production apiUrl=https://json-server-gamma-jet.vercel.app",
    "b:dev": "webpack --env mode=development",
    "l:ts": "npx eslint \"**/*.{ts,tsx}\"",
    "l:ts:fix": "npx eslint \"**/*.{ts,tsx}\" --fix",
    "l:scss": "npx stylelint \"**/*.scss\"",
    "l:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "pre:check": "npx prettier --check **/*.{ts,tsx,json}",
    "pre:write": "npx prettier --write **/*.{ts,tsx,json}",
    "t:unit": "jest --config ./config/jest/jest.config.ts",
    "t:e2e": "cypress open",
    "story": "storybook dev -p 6006 -c ./config/storybook",
    "story:b": "storybook build -c ./config/storybook -o dist-storybook",
    "ref": "storycap --outDir ./reports/storybook/screenshots/ref --serverCmd \"npx http-server dist-storybook -p 6006\" http://localhost:6006",
    "cur": "storycap --outDir ./reports/storybook/screenshots/current --serverCmd \"npx http-server dist-storybook -p 6006\" http://localhost:6006",
    "t:ui": "reg-cli ./reports/storybook/screenshots/current ./reports/storybook/screenshots/ref ./reports/storybook/screenshots/diff -R ./reports/storybook/index.html -J ./reports/storybook/report.json -M '0.3'",
    "prepare": "husky install",
    "gen:s": "node ./scripts/createSlice/index.js"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --list-different",
      "eslint"
    ]
  },
