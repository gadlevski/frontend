{
  "name": "frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "server": "node ./json-server/index.js",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "lint:ts": "npx eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "npx eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "t:unit": "jest --config ./config/jest/jest.config.ts",
    "story": "storybook dev -p 6006 -c ./config/storybook",
    "story:build": "storybook build -c ./config/storybook",
    "ref": "storycap --outDir ./__screenshots__/ref --serverCmd 'storybook dev -p 6008 -c ./config/storybook' http://localhost:6008",
    "cur": "storycap --outDir ./__screenshots__/current --serverCmd 'storybook dev -p 6008 -c ./config/storybook' http://localhost:6008",
    "t:ui": "reg-cli __screenshots__/current __screenshots__/ref __screenshots__/diff -R report.html -J report.json -M '0.3'",
    "prepare": "husky install"
  },
